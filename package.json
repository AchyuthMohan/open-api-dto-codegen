{
  "name": "openapi-dto-gen",
  "version": "1.0.0",
  "description": "TypeScript DTOs generated from OpenAPI spec",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "generate": "tsx scripts/generate-dtos.ts",
    "build": "tsc",
    "prepack": "npm run generate && npm run build"
  },
  "files": [
    "dist",
    "openapi"
  ],
  "keywords": [
    "openapi",
    "typescript",
    "dto",
    "notes-api"
  ],
  "author": "Achyuth Mohan",
  "license": "MIT",
  "devDependencies": {
    "openapi-typescript": "^6.7.5",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  },
  "peerDependencies": {
    "typescript": ">=5.0"
  },
  "sideEffects": false
}